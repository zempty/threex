# yá mǎi jiā
devMode=true
engDevMode=true

#项目名
projectName=all

dbtype=h2

# h2 datasource 在tcpserver指定了baseDir，这里就不用指定了。同时指定schema
# h2.jdbcurl=jdbc:h2:tcp://127.0.0.1:9123/./dbs/all;SCHEMA=ALL
h2.tcpPort=9123
h2.webPort=9122
h2.baseDir=./dbs
h2.jdbcurl=jdbc:h2:tcp://127.0.0.1:${h2.tcpPort}/all;SCHEMA=ALL
h2.username=sa
h2.password=123

#是否启用h2应用内的WebServer，建议关掉用第三方工具连接
enableWebServer=false

#变量
websocketPath=ws:192.168.1.199:8022

#是否允许应用启动完后打开浏览器
openBrowse=false
